{
  "name": "inhaus-bot",
  "version": "1.0.0",
  "description": "Discord bot to help run in-house events",
  "main": "dist/bot.js",
  "engines": {
    "node": "13.6"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --watch --config webpack.config.js",
    "start": "node dist/bot.js",
    "start:watch": "nodemon -r dotenv/config dist/bot.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solomontang/inhaus-bot.git"
  },
  "author": "Solomon Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solomontang/inhaus-bot/issues"
  },
  "homepage": "https://github.com/solomontang/inhaus-bot#readme",
  "dependencies": {
    "common-tags": "~1.8.0",
    "discord.js": "~11.5.1",
    "discord.js-commando": "~0.10.0",
    "knex": "~0.20.7",
    "pg": "~7.17.1"
  },
  "devDependencies": {
    "@types/common-tags": "~1.8.0",
    "@types/node": "~13.5.0",
    "babel-loader": "~8.0.6",
    "dotenv": "~8.2.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-config-prettier": "~6.9.0",
    "eslint-plugin-import": "~2.20.0",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-prettier": "~3.1.2",
    "fork-ts-checker-webpack-plugin": "~4.0.2",
    "prettier": "1.19.1",
    "ts-loader": "~6.2.1",
    "typescript": "~3.7.5",
    "webpack": "~4.41.5",
    "webpack-cli": "~3.3.10",
    "webpack-node-externals": "~1.7.2"
  }
}
